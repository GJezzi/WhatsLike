{
    "name": "WhatsLike",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "start": "node node_modules/react-native/local-cli/cli.js start",
        "test": "jest"
    },
    "dependencies": {
        "eslint-config-rallycoding": "^3.2.0",
        "firebase": "^4.9.1",
        "jshint": "^2.9.5",
        "react": "16.2.0",
        "react-native": "0.53.0",
        "react-native-router-flux": "git+https://github.com/aksonov/react-native-router-flux.git",
        "react-redux": "^5.0.6",
        "redux": "^3.7.2",
        "redux-thunk": "^2.2.0",
        "react-test-renderer": "^16.2.0"
    },
    "devDependencies": {
        "babel-jest": "^22.2.0",
        "babel-preset-react-native": "^4.0.0",
        "jest": "^22.2.0",
        "jest-react-native": "^18.0.0"
    },
    "jest": {
        "preset": "react-native",
        "transform": {
            "^.+\\.js?$": "babel-jest"
        },
        "moduleFileExtensions": ["js", "jsx"],
        "moduleDirectories": ["node_modules", "bower_components", "shared"],
        "moduleNameMapper": {
            "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$":
                "<rootDir>/tools/assetsTransformer.js",
            "\\.(css|less)$": "<rootDir>/tools/assetsTransformer.js"
        },
        "transformIgnorePatterns": ["!node_modules/react-runtime"]
    }
}
